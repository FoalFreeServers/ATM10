	"chapters/eternal_starlight.snbt": {
		quest.429785E8F64929CE.quest_desc: {
			new: ["Para chegar à Dimensão Starlight, você precisará encontrar Ruínas do Portal no Mundo Superior. \\n\\nLá você encontrará um Guardião do Portal, clique com o botão direito para iniciar uma conversa e desafiá-lo para um duelo! Cuidado, eles são muito difíceis. \\n\\nAssim que vencer o duelo, você receberá alguns Itens, sendo o Orbe da Profecia um deles. Clique com o botão direito no Portal com o Orbe para ativá-lo! Então, entre para chegar ao..."]
			old: ["Para chegar à Dimensão Starlight, você precisará encontrar Ruínas do Portal no Overworld. \\n\\nLá você encontrará um Guardião do Portal, clique com o botão direito para iniciar uma conversa e desafiá-lo para um duelo! Cuidado, eles são muito difíceis. \\n\\nAssim que vencer o duelo, você receberá alguns Itens, sendo o Orbe da Profecia um deles. Clique com o botão direito no Portal com o Orbe para ativá-lo! Então, entre para chegar ao..."]
		}
		quest.0576C32B0FB360B0.quest_desc: {
			new: [
				"A Monstruosidade Lunar tem 200 Corações e só receberá 1 de dano por golpe, a menos que você use Fogo.\\n\\nEla tem alguns ataques, incluindo Espinhos do Chão, Projéteis e Cabeças Emaranhadas Explosivas!\\n\\nQuando você a mata, ela dropa Flechas, Barras, Tangled Skulls, Pétalas Tenazes, Almas Aprisionadas e uma chance de uma Arma rara!"
				"{image:atm:textures/questpics/eternal/starlight_lunar.png width:100 height:125 align:center}"
			]
			old: [
				"A Monstruosidade Lunar tem 200 Corações e só receberá 1 de dano por golpe, a menos que você use Fogo.\\n\\nEla tem alguns ataques, incluindo Espinhos do Chão, Projéteis e Cabeças Emaranhadas Explosivas!\\n\\nQuando você a mata, ela dropa Flechas, Lingotes, Tangled Skulls, Pétalas Tenazes, Almas Aprisionadas e uma chance de uma Arma rara!"
				"{image:atm:textures/questpics/eternal/starlight_lunar.png width:100 height:125 align:center}"
			]
		}
		quest.0F4783FA019C4567.quest_desc: {
			new: ["Amaranto é encontrado raramente ao descascar Troncos de Torreya.\\n\\nPode ser transformado em Barras para fazer Armaduras e Ferramentas ou usado em outras receitas como uma fogueira que lhe dará regeneração!"]
			old: ["Amaranto é encontrado raramente ao descascar Troncos de Torreya.\\n\\nPode ser transformado em Lingotes para fazer Armaduras e Ferramentas ou usado em outras receitas como uma fogueira que lhe dará regeneração!"]
		}
		quest.3D26E7BFE0AA964A.quest_desc: {
			new: [
				"O Luminofish é um Peixe Luminoso que vive no Abismo.\\n\\nEles agem como Peixes normais. Você pode pescá-los, colocá-los em um Balde, e pode até cozinhá-los!"
				"{image:atm:textures/questpics/eternal/starlight_fish1.png width:100 height:100 align:center}"
			]
			old: [
				"O Luminofish é um peixe brilhante que vive no Abismo.\\n\\nEles agem como Peixes normais. Você pode pescá-los, colocá-los em um Balde, e pode até cozinhá-los!"
				"{image:atm:textures/questpics/eternal/starlight_fish1.png width:100 height:100 align:center}"
			]
		}
		quest.29A1E1FF02BD9219.title: {
			new: "Starlight Forest"
			old: "Floresta da Luz Estelar"
		}
		quest.3C74DAE7EC844A98.quest_desc: {
			new: ["As Bagas Lunares são uma excelente fonte de comida, além de serem muito fáceis de obter! \\n\\nElas crescem para baixo das Folhas de Árvore Lunar, você pode colher as Bagas das Vinhas como as Bagas Luminosass. \\nSó que estas dão muito mais Pontos de Fome e Saturação!"]
			old: ["As Bagas Lunares são uma excelente fonte de comida, além de serem muito fáceis de obter! \\n\\nElas crescem para baixo das Folhas de Árvore Lunar, você pode colher as Bagas das Vinhas como as Bagas Brilhantes. \\nSó que estas dão muito mais Pontos de Fome e Saturação!"]
		}
