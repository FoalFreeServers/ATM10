		quest.6852BEFDD4DE4E4B.quest_desc: {
			new: ["&9Estantes Profundas Tocadas pela Alma&r são uma melhoria para as &9Estantes Profundas&r. \\n\\nElas são Criadas com uma &bLanterna de Alma&r e 3 &3Blocos de Sculk&r. Você precisará de &5Toque Suave&r para pegar os &3Blocos de Sculk&r. \\n\\nEntão, podemos Criar nossa versão &9Tocada pela Alma&r. Elas darão muito mais &cQuanta&r, com &c15%&r! \\n\\nTambém aumenta a &aEterna&r máxima para &a75&r."]
			old: ["&9Estantes Profundas Tocadas pela Alma&r são uma melhoria para as &9Estantes Profundas&r. \\n\\nElas são Criadas com uma &bLanterna de Alma&r e 3 &3Blocos de Sculk&r. Você precisará de &5Silk-Touch&r para pegar os &3Blocos de Sculk&r. \\n\\nEntão, podemos Criar nossa versão &9Tocada pela Alma&r. Elas darão muito mais &cQuanta&r, com &c15%&r! \\n\\nTambém aumenta a &aEterna&r máxima para &a75&r."]
		}
		quest.24391162F4FF0587.quest_desc: {
			new: ["Com as 15 &bEstantes Marinhas&r ao redor da &5Mesa de Encantamento&r, podemos &d&lInfundir&r nossa última &bEstante Marinha&r! \\n\\n&bEstantes Marinhas Infundidas&r dão mais &aEterna&r, 5, com um máximo de &a60&r. Também mais &5Arcana&r, &55%&r. \\n\\nElas podem ser transformadas em diferentes &7Estantes de Livros&r, as que estamos procurando para o &6Encantamento Máximo&r são as &bEstantes Marinhas Cristalinas&r. \\n\\nPrecisaremos de 12 delas, então precisaremos de 12 &bEstantes Marinhas Infundidas&r. Crie 8 e eu te recompensarei com o resto!"]
			old: ["Com as 15 &bEstantes Marinhas&r ao redor da &5Enchantment Table&r, podemos &d&lInfundir&r nossa última &bEstante Marinha&r! \\n\\n&bEstantes Marinhas Infundidas&r dão mais &aEterna&r, 5, com um máximo de &a60&r. Também mais &5Arcana&r, &55%&r. \\n\\nElas podem ser transformadas em diferentes &7Estantes de Livros&r, as que estamos procurando para o &6Encantamento Máximo&r são as &bEstantes Marinhas Cristalinas&r. \\n\\nPrecisaremos de 12 delas, então precisaremos de 12 &bEstantes Marinhas Infundidas&r. Crie 8 e eu te recompensarei com o resto!"]
		}
		quest.60850E717C747A78.quest_desc: {
			new: ["Você já passou 3 meses estudando &oCódigo Galáctico&r para finalmente entender a linguagem da &5Mesa de Encantamento&r só para descobrir que era um monte de besteira? \\n\\nNão? \\n\\nEu também não, mas as &9Pistas de Encantamento&r são seu tradutor real para isso! \\n\\nCada &9Pista de Encantamento&r lhe dirá 1 &5Encantamento&r antes de você realmente usá-lo."]
			old: ["Você já passou 3 meses estudando &oCódigo Galáctico&r para finalmente entender a linguagem da &5Enchantment Table&r só para descobrir que era um monte de besteira? \\n\\nNão? \\n\\nEu também não, mas as &9Pistas de Encantamento&r são seu tradutor real para isso! \\n\\nCada &9Pista de Encantamento&r lhe dirá 1 &5Encantamento&r antes de você realmente usá-lo."]
		}
		quest.1873205CF5247E3E.quest_desc: {
			new: [
				"A última e maior das &7Estantes&r, as &dEstantes do End&r! \\n\\nElas são criadas com 6 &eTijolos de Endstone&r, 1 &3Pérola do End&r, 1 &7Estante de Livros&r e, claro, o &dBafo de Dragão Infundido&r. \\n\\nSeus status não são os melhores atualmente, dando apenas 5 para cada. Mas seu máximo de &aEterna&r de &a90&r é incrível! \\n\\nAh, sim, e elas podem ser Criadas com outros Itens para atualizá-las!"
				"{image:atm:textures/questpics/apothic/enchant_endshelf.png width:100 height:100 align:center}"
			]
			old: [
				"A última e maior das &7Estantes&r, as &dEstantes do End&r! \\n\\nElas são criadas com 6 &eTijolos de Endstone&r, 1 &3Pérola do End&r, 1 &7Bookshelf&r e, claro, o &dBafio de Dragão Infundido&r. \\n\\nSeus status não são os melhores atualmente, dando apenas 5 para cada. Mas seu máximo de &aEterna&r de &a90&r é incrível! \\n\\nAh, sim, e elas podem ser Criadas com outros Itens para atualizá-las!"
				"{image:atm:textures/questpics/apothic/enchant_endshelf.png width:100 height:100 align:center}"
			]
		}
		quest.45161821C221EF0B.quest_desc: {
			new: ["Assim que tivermos essas 15 &4Hellshelves&r ao redor da nossa &5Mesa de Encantamento&r, teremos &a45 Eterna&r e &c30% Quanta&r na nossa &5Mesa&r. \\n\\nEsta é a quantidade perfeita para &d&lInfundir&r nossas &4Hellshelves&r! \\n\\nAs &4Estantes Infernais Infundidas&r dão &a5 Eterna&r com um máximo de &a60 Eterna&r, junto com &c5% Quanta&r. \\n\\nElas podem ser Fabricadas com outros Itens para fazer diferentes &7Estantes de Livros&r, para as que queremos, precisaremos de 12 &4Estantes Infernais Infundidas&r! \\n\\nClaro, como sou muuuito generoso, você só precisará coletar 8!"]
			old: ["Assim que tivermos essas 15 &4Hellshelves&r ao redor da nossa &5Enchantment Table&r, teremos &a45 Eterna&r e &c30% Quanta&r na nossa &5Mesa&r. \\n\\nEsta é a quantidade perfeita para &d&lInfundir&r nossas &4Hellshelves&r! \\n\\nAs &4Hellshelves Infundidas&r dão &a5 Eterna&r com um máximo de &a60 Eterna&r, junto com &c5% Quanta&r. \\n\\nElas podem ser Fabricadas com outros Itens para fazer diferentes &7Estantes de Livros&r, para as que queremos, precisaremos de 12 &4Hellshelves Infundidas&r! \\n\\nClaro, como sou muuuito generoso, você só precisará coletar 8!"]
		}
		quest.0518551637F76C50.quest_desc: {
			new: ["Para a melhoria &9Ecoante&r, precisaremos de alguns Itens diferentes.\\n\\Velas, que são relativamente fáceis de obter! E &9Fragmentos de Eco&r... que definitivamente não são...\\n\\nA melhor chance de obtê-los é saqueando uma &6Cidade Ancestral&r, mas você pode criá-los com alguns Rituais diferentes.\\n\\n(Ou até mais tarde com &5Encantamento&r!)\\n\\nA &9Estante Profunda Ecoante&r dará muito mais &5Arcana&r, com &515%&r! Além de aumentar o máximo de &aEterna&r para &a75&r."]
			old: ["Para a melhoria &9Ecoante&r, precisaremos de alguns Itens diferentes.\\n\\Velas, que são relativamente fáceis de obter! E &9Fragmentos de Eco&r... que definitivamente não são...\\n\\nA melhor chance de obtê-los é saqueando uma &6Ancient City&r, mas você pode criá-los com alguns Rituais diferentes.\\n\\n(Ou até mais tarde com &5Encantamento&r!)\\n\\nA &9Estante Profunda Ecoante&r dará muito mais &5Arcana&r, com &515%&r! Além de aumentar o máximo de &aEterna&r para &a75&r."]
		}
		quest.0A3C464EF7CDD5F6.quest_desc: {
			new: [
				"As &4Hellshelves&r são sua introdução à &cQuanta&r, elas dão &c3% de Quanta&r e &a3 de Eterna&r.\\n\\nMelhor que as Estantes normais, certo?\\n\\nVocê vai querer 16 delas para o próximo passo, 15 &4Hellshelves&r ao redor da &5Mesa de Encantamento&r, com 1 no seu Inventário.\\n\\nFaça as primeiras 11 e eu lhe darei as últimas 5 &4Hellshelves&r!"
