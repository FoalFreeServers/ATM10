	quest.67DBE6C59C0D9D1B.title: "&4Supremium Essence&r"
	quest.67DDFA6FB1F9EECA.title: "&9Imperium Tools and Weapons"
	quest.680ECE3D8DCDC515.quest_subtitle: "&f13&r &4Damage&r"
